public class Queue{
    Node erstes;
    Node letztes;
    
    // Note: kann man eigentlich komplett weglassen (Standard Konstruktor)
    public Queue(){
        this.erstes = null;
        this.letztes = null;
    }

    
    public void enqueue(Bestellbest pBestellbest){
        Node pNode = new Node(pBestellbest);
        if (isEmpty()) {
            this.erstes = pNode;
        } else {
            this.letztes.setNF(pNode);
        } // end of if-else
        this.letztes = pNode;
    }
    
    public void dequeue(){
        if (!isEmpty()) {
            this.erstes = this.erstes.getNF();
            if (this.erstes == null) {
                this.letztes = null;
            } // end of if

        } // end of if
    }

    public boolean isEmpty(){
        if (this.erstes == null) {
            return true;
        } // end of if
        return false;
    }

    public Node getFirst(){
        return this.erstes;
    }


}
